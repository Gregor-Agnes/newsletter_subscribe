{
    "name": "zwo3/newsletter_subscribe",
    "type": "typo3-cms-extension",
    "description": "Subscribe / Unsubscribe Extension (e.g. for Newsletter), depends on tt_address",
    "homepage": "https://github.com/Gregor-Agnes/newsletter_subscribe",
    "license": [
        "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Gregor Agnes",
            "role": "Developer",
            "email": "ga@zwo3.de"
        }
    ],
    "keywords": [
        "TYPO3",
        "extension",
        "tt_address",
        "subscribe",
        "newsletter"
    ],
    "require": {
        "typo3/cms-core": "^v11.5.16",
        "friendsoftypo3/tt-address": "^6.1.0 || ^7.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "ssch/typo3-encore": "^v4.0.3"
    },
    "autoload": {
        "psr-4": {
            "Zwo3\\NewsletterSubscribe\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zwo3\\NewsletterSubscribe\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "newsletter_subscribe"
        }
    }
}
